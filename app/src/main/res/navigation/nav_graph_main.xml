<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/fragMain"
    tools:ignore="UnusedNavigation">
    <fragment
        android:id="@+id/fragMain"
        android:name="com.haichenyi.community.ui.frag.FragMain"
        android:label="FragMain"
        tools:layout="@layout/frag_main">
        <action
            android:id="@+id/to_frag_test"
            app:destination="@id/fragTest"
            app:enterAnim="@anim/slide_enter_anim"
            app:exitAnim="@anim/slide_exit_anim"
            app:popEnterAnim="@anim/slide_pop_enter_anim"
            app:popExitAnim="@anim/slide_pop_exit_anim" />
        <action
            android:id="@+id/to_frag_home"
            app:destination="@id/fragHome"
            app:enterAnim="@anim/slide_enter_anim"
            app:exitAnim="@anim/slide_exit_anim"
            app:popEnterAnim="@anim/slide_pop_enter_anim"
            app:popExitAnim="@anim/slide_pop_exit_anim" />
        <action
            android:id="@+id/to_frag_author"
            app:destination="@id/fragAuthor"
            app:enterAnim="@anim/slide_enter_anim"
            app:exitAnim="@anim/slide_exit_anim"
            app:popEnterAnim="@anim/slide_pop_enter_anim"
            app:popExitAnim="@anim/slide_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/fragTest"
        android:name="com.haichenyi.community.ui.frag.FragTest"
        android:label="FragTest"
        tools:layout="@layout/frag_test" />
    <fragment
        android:id="@+id/fragHome"
        android:name="com.haichenyi.community.ui.frag.FragHome"
        android:label="FragHome"
        tools:layout="@layout/frag_home">

    </fragment>
    <fragment
        android:id="@+id/fragAuthor"
        android:name="com.haichenyi.community.ui.frag.FragAuthor"
        android:label="FragAuthor"
        tools:layout="@layout/frag_author" />

</navigation>